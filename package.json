{
  "name": "mixshift-sqp-api",
  "version": "2.0.0",
  "description": "Optimized Amazon SP-API SQP (Search Query Performance) Cron Service",
  "main": "src/server.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src --ext js,json --exec node src/server.js",
    "serve": "NODE_ENV=production node src/server.js",
    "test": "jest --runInBand --coverage",
    "test:unit": "jest --runInBand tests/unit",
    "test:integration": "jest --runInBand tests/integration",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand --collectCoverageFrom='src/**/*.js'",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write 'src/**/*.js'",
    "validate": "npm run lint && npm run test"
  },
  "keywords": [
    "amazon-sp-api",
    "search-query-performance",
    "cron",
    "brand-analytics",
    "multi-tenant"
  ],
  "author": "MaxShift Team",
  "license": "ISC",
  "dependencies": {
    "@amazon-sp-api-release/amazon-sp-api-sdk-js": "^1.4.0",
    "@aws-sdk/client-s3": "^3.645.0",
    "aws4": "^1.13.2",
    "axios": "^1.11.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.1",
    "nodemailer": "^7.0.6",
    "pino": "^9.9.4",
    "pino-pretty": "^13.1.1",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.10"
  }
}
